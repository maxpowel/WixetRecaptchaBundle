    "description": "This Bundle provides support for google recaptcha into symfony forms",
    "keywords": ["validation","recaptcha","form"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Alvaro Garcia",
            "email": "maxpowel@gmail.com"
        }
    ],

    "require": {
        "php": "^5.5.9|^7.0",
        "twig/twig": "~1.20|~2.0",
        "symfony/framework-bundle": "~2.3|~3.0"
    },

    "require-dev": {
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/twig-bundle": "~2.3|~3.0",
        "symfony/browser-kit": "~2.3|~3.0"
    },

    "autoload": {
        "psr-4": { "Wixet\\RecaptchaBundleBundle\\": "" }
    }

}